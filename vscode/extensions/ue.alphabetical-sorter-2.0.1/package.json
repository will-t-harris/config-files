{
	"name": "alphabetical-sorter",
	"displayName": "Alphabetical Sorter",
	"description": "Multi line or single line alphabetical sorter.",
	"version": "2.0.1",
	"icon": "images/icon.png",
	"publisher": "ue",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/ue/alphabetical-sorter.git"
	},
	"bugs": {
		"url": "https://github.com/ue/alphabetical-sorter/issues",
		"email": "ugurerdal93@gmail.com"
	},
	"engines": {
		"vscode": "^1.20.0"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onCommand:extension.sortIt"
	],
	"main": "./src/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.sortIt",
				"title": "Alphabetical Sorter"
			}
		],
		"keybindings": [
			{
				"command": "extension.sortIt",
				"key": "ctrl+shift+a",
				"mac": "cmd+shift+a",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.6.1",
		"vscode": "^1.1.6",
		"eslint": "^4.11.0",
		"@types/node": "^7.0.43",
		"@types/mocha": "^2.2.42"
	},
	"__metadata": {
		"id": "914c8b2e-3129-4570-860b-8ab8c51905e0",
		"publisherId": "fdf2364b-9bd7-4ab7-aada-ceb0d83004f3",
		"publisherDisplayName": "Ugur Erdal"
	}
}